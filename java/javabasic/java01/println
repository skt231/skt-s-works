package one;

public class Java01 {

	public static void main(String[] args) {
		// 일반적으로 자바는 한줄에 하나의 명령어를 사용한다.
		// 여러줄로 코드를 작성하고 싶으면
		// , = . 괄호 다음에는 줄을 바꿔 쓸수 있다.		
		int a=10;
		System.out.println(a);
		int b=10,c=20;
		System.out.println(b);
		System.out.println(c);
		
		int d=
				10,
		f=20;
		System.out.println(d);
		System.out.println(f);

	}

}

